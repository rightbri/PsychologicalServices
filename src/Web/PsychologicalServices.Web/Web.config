  </connectionStrings>
  <appSettings file="AppSettingsSecrets.config">
    <add key="HtmlToPdf.ExecutablePath" value="wkhtmltopdf.exe" />
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="log4net.Internal.Debug" value="true" />
  </appSettings>
  <log4net>
    <appender name="ApplicationLogAppender" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <file value="..\logs\" />
      <datePattern value="'log.'yyyy-MM-dd'.txt'" />
      <staticLogFileName value="false" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <immediateFlush value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-5p [%d{yyyy-MM-dd HH:mm:ss}] %ndc - %m%n" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="INFO" />
        <levelMax value="FATAL" />
      </filter>
    </appender>
    <appender name="AdoNetAppender" type="log4net.Appender.AdoNetAppender">
      <bufferSize value="1" />
      <threshold value="ALL" />
      <connectionType value="System.Data.SqlClient.SqlConnection, System.Data, Version=4.0.0.0, Culture=neutral,  PublicKeyToken=b77a5c561934e089" />
      <connectionStringName value="PsychologicalServices" />
      <commandText value="INSERT INTO [dbo].[Log]([Date], [Message],[Level],[Logger],[Username],[Exception]) VALUES(@Date, @Message, @Level, @Logger, @Username, @Exception)" />
      <commandType value="Text" />
      <parameter>
        <parameterName value="@message" />
        <dbType value="AnsiString" />
        <size value="-1" />
        <layout type="log4net.Layout.PatternLayout" value="%message" />
      </parameter>
      <parameter>
        <parameterName value="@date" />
        <dbType value="DateTime" />
        <layout type="log4net.Layout.RawTimeStampLayout" />
      </parameter>
      <parameter>
        <parameterName value="@level" />
        <dbType value="AnsiString" />
        <size value="50" />
        <layout type="log4net.Layout.PatternLayout" value="%level" />
      </parameter>
      <parameter>
        <parameterName value="@logger" />
        <dbType value="AnsiString" />
        <size value="255" />
        <layout type="log4net.Layout.PatternLayout" value="%logger" />
      </parameter>
      <parameter>
        <parameterName value="@userName" />
        <dbType value="AnsiString" />
        <size value="255" />
        <layout type="log4net.Layout.PatternLayout" value="%identity" />
      </parameter>
      <parameter>
        <parameterName value="@exception" />
        <dbType value="AnsiString" />
        <size value="-1" />
        <layout type="log4net.Layout.ExceptionLayout" />
      </parameter>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="DEBUG" />
        <levelMax value="FATAL" />
      </filter>
    </appender>
    <appender name="SmtpAppender" type="log4net.Appender.SmtpAppender">
      <to value="brian.avent@gmail.com" />
      <from value="PsychologicalServices&lt;watsonpsychologicalservices@gmail.com&gt;" />
      <subject value="Psychological Services - Application Error (DEVELOPMENT)" />
      <priority value="High" />
      <smtpHost value="smtp-gw.nas.medcity.net" />
      <bufferSize value="1" />
      <lossy value="true" />
      <evaluator type="log4net.Core.LevelEvaluator">
        <threshold value="ERROR" />
      </evaluator>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="Level: %-5level%newlineHostname: %-5[%property{host}]%newlineMessage: %-5message%newlineUserName: %-5username%newlineDate/Time: %-5%date%newlineLogger:%-5logger%newline%exception%newline" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR" />
        <levelMax value="FATAL" />
      </filter>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="ApplicationLogAppender" />
      <appender-ref ref="AdoNetAppender" />
    </root>
  </log4net>
